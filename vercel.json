{
  "version": 2,
  "builds": [
    { "src": "server.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/server.js" }
  ],
  "install": {
    "includeFiles": ["**/*"],
    "includeCommands": ["npm install"]
  },
  "builds": [
    { 
      "src": "/api/index.ts",
      "use": "@now/node"
    }
  ]
}